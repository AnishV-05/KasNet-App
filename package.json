{
  "name": "reg-kasnetdigital-dashboardtransaction-web",
  "description": "Dashboard transaction Microfrontend",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "/",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist/**/*.js",
    "dist/**/*.txt",
    "dist/**/*.css"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "serve": "serve dist -p 3300 -s --cors",
    "test": "jest -o --passWithNoTests --detectOpenHandles --reporters='default'",
    "test:all": "jest --passWithNoTests --detectOpenHandles",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development --live-reload",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"infra/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\" \"infra/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\" -f stylish",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "generate-api": "cd ./scripts && npx @rtk-query/codegen-openapi api-generator.ts",
    "prebuild": "rimraf dist",
    "preserve": "node scripts/copy-tokens.js",
    "prepare": "husky install"
  },
  "release": {
    "branches": [
      "production"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md",
          "changelogTitle": "# Changelog"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "successCmd": "replace '([A-Za-z]+-[0-9]+):' '[$1](https://kasnet.atlassian.net/browse/$1):' docs/CHANGELOG.md"
        }
      ]
    ]
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.3.2",
    "@pulumi/aws": "^5.42.0",
    "@pulumi/awsguard": "^0.4.0",
    "@pulumi/awsx": "^2.12.3",
    "@pulumi/policy": "^1.11.0",
    "@pulumi/pulumi": "^3.121.0",
    "@reduxjs/toolkit": "1.9.7",
    "@tanstack/react-table": "^8.21.3",
    "amplitude-js": "7.3.0",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "classnames": "^2.5.1",
    "dotenv-webpack": "^7.1.1",
    "esbuild-loader": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "i18next": "^23.6.0",
    "jose": "^5.2.0",
    "luxon": "^3.6.1",
    "process": "^0.11.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-i18next": "^13.3.1",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.17.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "styled-components": "^5.3.0",
    "timers-browserify": "^2.0.12",
    "yup": "^1.3.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@pulumi/aws": "^6.6.1",
    "@pulumi/awsguard": "latest",
    "@pulumi/awsx": "^2.0.3",
    "@pulumi/policy": "^1.8.0",
    "@pulumi/pulumi": "^3.91.0",
    "@rtk-query/codegen-openapi": "1.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/amplitude-js": "^6.0.0",
    "@types/jest": "^29.5.6",
    "@types/jsrsasign": "^10.5.12",
    "@types/luxon": "^3",
    "@types/node": "^20.8.9",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/react-redux": "^7.1.33",
    "@types/styled-components": "^5.1.29",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "8.32.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
