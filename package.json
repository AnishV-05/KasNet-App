    "react-test-renderer": "^19.1.0",
    "replace": "^1.2.2",
    "sass": "1.77.8",
    "sass-loader": "16.0.0",
    "semantic-release": "19.0.5",
    "serve": "^14.2.1",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.1"
  },
  "packageManager": "yarn@4.5.3",
  "resolutions": {
    "eslint-config-prettier": "10.1.5",
    "@pkgr/core": "0.1.1",
    "synckit": "0.9.1"
  }
}
